
#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int num1, int num2) {
    int answer = 0;
    answer=num1/num2;
    return answer;
}

int main(void){
    int num1=0, num2=0;

    printf("|num1|num2|num3|");
    num1= 10, num2= 5;
    printf("|%d|%d|%d|",num1,num2,solution(num1,num2));
    num1= 7, num2= 2;
    printf("|%d|%d|%d|",num1,num2,solution(num1,num2));
}
